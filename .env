# Налаштування для Playwright тестів

# Збереження скріншотів на диск
# true - зберігати PNG файли в test-results/{test-name}/step_*.png
# false - тільки base64 в JSON звіті
SAVE_SCREENSHOTS=false

# Вивід детальних результатів тестів в консоль
# true - показувати деталі по кожному тесту та кроку
# false - тільки стандартний вивід Playwright
PRINT_TEST_RESULTS=true
